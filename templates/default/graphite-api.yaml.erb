cache:
  dir: <%= @cache_dir %>
  type: <%= @cache_type %>
finders: <% @finders.each do |f| %>
- <%= f %>
<% end -%>
functions: <% @functions.each do |f| %>
- <%= f %>
<% end -%>
<% if @kairosdb -%>
kairosdb:
  uri: http://<%= @kairosdb_host %>:<%= @kairosdb_port %>
  es:
    url: http://<%= @elasticsearch_host %>:<%= @elasticsearch_port %>/
<% end -%>
search_index: <%= @search_index %>
time_zone: <%= @time_zone %>
<% if @use_statsd %>
statsd:
  host: '<%= @statsd_host %>'
  port: '<%= @statsd_port %>'
<% end -%>
